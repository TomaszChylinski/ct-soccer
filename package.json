{
		"name": "ct-soccer",
		"version": "1.0.0",
		"description": "Application built for the purpose to update fans about CT soccer",
		"main": "index.js",
		"scripts": {
				"test": "echo \"Error: no test specified\" && exit 1",
				"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev ",
				"start:prod": "node server.js",
				"start:dev": "concurrently  \"nodemon --ignore 'client/*'\" \"npm run client\"",
				"client": "cd client && npm run start",
				"install": "cd client && npm install",
				"build": "cd client && npm run build"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/TomaszChylinski/ct-soccer.git"
		},
		"author": "Tomasz Chylinski",
		"license": "ISC",
		"bugs": {
				"url": "https://github.com/TomaszChylinski/ct-soccer/issues"
		},
		"homepage": "https://github.com/TomaszChylinski/ct-soccer#readme",
		"dependencies": {
				"axios": "^0.19.2",
				"bcryptjs": "^2.4.3",
				"cheerio": "^1.0.0-rc.3",
				"cors": "^2.8.5",
				"express": "^4.17.1",
				"express-session": "^1.17.0",
				"if-env": "^1.0.4",
				"jsonwebtoken": "^8.5.1",
				"mongodb": "^3.5.5",
				"mongoose": "^5.9.5",
				"morgan": "^1.10.0",
				"passport": "^0.4.1",
				"passport-local": "^1.0.0",
				"react-router-dom": "^5.1.2"
		},
		"devDependencies": {
				"concurrently": "^5.1.0",
				"nodemon": "^2.0.2"
		}
}
